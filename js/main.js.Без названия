function addString() {
    i = typeof data[i] != 'undefined' ? i : 0;
    var p = $('<p />').appendTo(cons).append(cursor.removeClass('blink'));
    if (cons[0].scrollHeight > consH) {
        $('p:first-child', cons).remove();  
    }
    printer(data[i++]);
    return false;
}
