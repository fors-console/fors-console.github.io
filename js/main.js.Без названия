function printer(d) {
    var l = d[0].length,
        c = 0,
        timer = setInterval(function () {
            if (c > l) {
                clearInterval(timer);
                cursor.addClass('blink');
                setTimeout(addString, d[1]);
            }
            cursor.before(d[0][c++]);
        }, 0);
    return false;
}
