function user(){
 
	this.Auth = function(){
		$.ajax({
			"url": "../../api/auth.php", 
			"type": "POST", 
			"dataType": "html",
			"data": $("#auth").serialize(),  
			"success": function(html) {
				if(html == 'Успешно'){location.reload();}
				$(".response").fadeOut();
				$(".response").fadeIn().html(html); 
			},
		}); 
	}
	
	this.Console = function(act){
		switch(act){
			case 'open':     
			$( ".consolestart" ).dblclick(function() {
				$('html').css('cursor', 'wait');
				setTimeout(function() {
					$('.console').show();
					$('html').css('cursor', 'default');
					$('.console p').remove();
				}, 3000);
			});

			break;
			case 'close':
				$('.console').hide(); 
			break;
		}
	}

} 
var User = new user(); 

